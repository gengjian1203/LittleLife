/**app.wxss**/
.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
} 

button {
  background: initial;
}

button:focus{
  outline: 0;
}

button::after{
  border: none;
}

/* iconfont */
@font-face {
  font-family: 'iconfont';  /* project id 1439371 */
  src: url('//at.alicdn.com/t/font_1439371_wxmryj7yxl.eot');
  src: url('//at.alicdn.com/t/font_1439371_wxmryj7yxl.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_1439371_wxmryj7yxl.woff2') format('woff2'),
  url('//at.alicdn.com/t/font_1439371_wxmryj7yxl.woff') format('woff'),
  url('//at.alicdn.com/t/font_1439371_wxmryj7yxl.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_1439371_wxmryj7yxl.svg#iconfont') format('svg');
}

.iconfont{
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;
  }

.icon_search::before {
  content: "\e61a"
}

.icon_house::before {
  content: "\e608"
}

.icon_car::before {
  content: "\e61d"
}

.icon_boss::before {
